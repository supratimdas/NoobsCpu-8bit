##this is a testlist, with a bunch of assembly code trying to exercise various scenarios.
#test-name, asm file, description
test_load_store, test_load_store.asm, "basic single load from memory followed by single store in memory"
test_add, test_add.asm, "load 2 data from memory add them and store to a destination memory location"
test_sub, test_sub.asm, "load 2 data from memory subtract them and store to a destination memory location"
test_or, test_or.asm, "load 2 data from memory or them and store to a destination memory location"
test_and, test_and.asm, "load 2 data from memory and them and store to a destination memory location"
test_xor, test_xor.asm, "load 2 data from memory xor them and store to a destination memory location"
test_unconditional_jmp, test_unconditional_jmp.asm, "simple unconditional jump"
test_conditional_jmpz_false, test_conditional_jmpz_false.asm, "conditional jump if zero, condition evaluates true"
test_conditional_jmpz_true, test_conditional_jmpz_true.asm, "conditional jump if zero, condition evaluates false"
test_conditional_jmpnz_false, test_conditional_jmpnz_false.asm, "conditional jump if not zero, condition evaluates true"
test_conditional_jmpnz_true, test_conditional_jmpnz_true.asm, "conditional jump if not zero, condition evaluates false"
test_loop, test_loop.asm, "a simple loop"
multiply, multiply.asm, "a simple multiplication implementation"
fibonacci, fibonacci.asm, "compute the nth fibonacci number"
array_copy, test_array_copy.asm, "to copy an array using loop"
test_alternate_reg_access,test_alternate_reg_access.asm, "alternate access of registers using reserved address 0-6"
